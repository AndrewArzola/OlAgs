<p id="notice"><%= notice %></p>

<h1>Events</h1>

<th>Calendar View</th>

<%= month_calendar(events: @events) do |date, events| %>
  <%= date %>

  <% events.each do |event| %>
    <div>
      <%= link_to event.name, event%>
    </div>
  <% end %>
<% end %>
<br>

<%= link_to 'Add New Event', new_event_path %>