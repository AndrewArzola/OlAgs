<%= search_form_for @q do |f| %>
  <div class="container">
    <div class="row">
      <div class="col">
        <%= f.label :fname_or_lname_cont %>
        <%= f.search_field :fname_or_lname_cont %>
      </div>
      <div class="col">
        <%= f.label :email_cont %>
        <%= f.search_field :email_cont %>
      </div>
      <div class="col">
        <%= f.label :major_cont %>
        <%= f.search_field :major_cont %>
      </div>
      <div class="col">
        <%= f.label :active_eq %>
        <%= f.search_field :active_eq %>
      </div>
      <div class = "col">
        <%= f.submit %>
      </div>
    </div>
  </div>                    
  <% end %>