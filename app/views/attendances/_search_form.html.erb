<%= search_form_for @q, url: search_attendances_path,
    html: { method: :post} do |f| %>
    <div class="container">
    <div class="row justify-content-end">
      <div class="col">
        <%= f.label :member_full_name_cont, "Member Name" %>
        <%= f.search_field :member_full_name_cont %>
      </div>
      <div class="col">
        <%= f.label :event_name_cont, "Event Name"%>
        <%= f.search_field :event_name_cont %>
      </div>
      <div class="col">
          <%= f.label :attended_eq, "Attended" %>
    <%= f.search_field :attended_eq, placeholder:"true/false"%>
      </div>
      <div class="col">    
        <%= f.label :rsvp_eq, "RSVP" %>
        <%= f.search_field :rsvp_eq, placeholder:"true/false" %>
      </div>
      <div class = "col">
        <%= f.submit %>
      </div>
    </div>
  </div>  
<% end %>